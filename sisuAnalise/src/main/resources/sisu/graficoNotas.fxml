<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sisu.GraficoNotasController">
   <children>
      <BarChart fx:id="barChart" AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
            <xAxis>
                <CategoryAxis fx:id="xAxis" side="BOTTOM" label="ano" />
            </xAxis>
            <yAxis>
                <NumberAxis side="LEFT" fx:id="yAxis" label="nota"/>
            </yAxis>
      </BarChart>
   </children>
</AnchorPane>
